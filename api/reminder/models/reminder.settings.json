{
  "connection": "default",
  "collectionName": "reminders",
  "info": {
    "name": "Reminder"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "is_activated": {
      "type": "boolean",
      "default": true
    },
    "time": {
      "type": "string"
    },
    "user_id": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
