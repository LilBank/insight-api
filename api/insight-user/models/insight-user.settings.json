{
  "connection": "default",
  "collectionName": "insight_users",
  "info": {
    "name": "InsightUser"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "date_of_birth": {
      "type": "date"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone_number": {
      "type": "integer",
      "required": true
    },
    "image_link": {
      "type": "string"
    },
    "address": {
      "type": "richtext",
      "required": true
    },
    "role": {
      "type": "integer"
    },
    "venues": {
      "collection": "venue"
    },
    "activity_id": {
      "model": "activity",
      "via": "insight_user_id"
    },
    "playlists": {
      "collection": "playlist"
    },
    "reminders": {
      "collection": "reminder"
    }
  }
}
