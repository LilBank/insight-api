{
  "connection": "default",
  "collectionName": "activities",
  "info": {
    "name": "Activity"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "start_time": {
      "type": "string",
      "required": true
    },
    "end_time": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "user_id": {
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
